<?xml version="1.0"?>
<!DOCTYPE plugin SYSTEM "manifest.dtd">
<plugin version="2.1">
	<info>
		<version>0.3</version>
		<title>Kopano Developer Plugin</title>
		<author>Kopano</author>
		<authorURL>https://www.kopano.com</authorURL>
		<description>Shows all available insertion points in the Kopano WebApp</description>
	</info>
	<components>
		<component>
			<files>
				<client>
					<clientfile load="release">js/zdeveloper.js</clientfile>
					<clientfile load="debug">js/zdeveloper-debug.js</clientfile>
					<clientfile load="source">js/ZDeveloper.js</clientfile>
				</client>
				<resources>
					<resourcefile load="release">resources/css/zdeveloper.css</resourcefile>
					<resourcefile load="source">resources/css/zdeveloper-styles.css</resourcefile>
				</resources>
			</files>
		</component>
	</components>
</plugin>
